{
  "name": "my-portfolio-website",
  "version": "1.0.0",
  "description": "My portfolio website.",
  "main": "index.js",
  "scripts": {
    "compile:less": "lessc ./less/main.less ./assets/css/style.css",
    "nodemon:compile:less": "nodemon -e less -x \"npm run compile:less\"",
    "prefix:css": "postcss --use autoprefixer -b 'last 5 versions' assets/css/style.css -o assets/css/style.prefix.css",
    "bundle:js": "rollup ./assets/js/home.js --file ./assets/js/home.packed.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jirkasa/my-portfolio-website.git"
  },
  "author": "Jiří Satora",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jirkasa/my-portfolio-website/issues"
  },
  "homepage": "https://github.com/Jirkasa/my-portfolio-website#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.4",
    "less": "^4.1.2",
    "nodemon": "^2.0.15",
    "postcss": "^8.3.11",
    "postcss-cli": "^9.0.2",
    "rollup": "^2.70.1"
  }
}
